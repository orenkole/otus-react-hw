  "lint-staged": {
    "*.{js,ts,tsx, jsx}": [
      "eslint ./src --fix"
    ],
    "*.{js, ts, tsx, jsx}": "eslint --cache --fix",
    "*.{ts, tsx, js, jsx, css,md}": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!./src/index.tsx"
    ],
    "coveragePathIgnorePatterns": [
      "^.*.style.[jt]sx?$",
      "^.*.types.[jt]sx?$",
      "^.*.stories.[jt]sx?$"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/settings/JestSetup.js"
    ]
  }
